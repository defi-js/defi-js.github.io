{
  "name": "defi-js.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.27",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "5.x",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-router-dom": "5.x",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2",
    "@defi.org/web3-candies": "2.x",
    "@mui/material": "5.x",
    "@mui/icons-material": "5.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "typechain": "typechain --target=web3-v1 --out-dir=typechain-abi './src/abi/**/*.json'",
    "build": "rm -rf docs build typechain-abi && npm run typechain && react-scripts build && cp -r ./build ./docs",
    "serve": "serve -s ./docs",
    "test": "react-scripts test",
    "deploy": "aws s3 cp --recursive --acl public-read ./docs/ s3://defi-org/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 180
  }
}
